    <ExtraParams/>
    <PostArgs/>
    <Config Name="SqlLite" Target="/db" Default="" Mode="rw" Description="Path to sqllite db" Type="Path" Display="always" Required="false" Mask="false"/>
    <Config Name="HTTP" Target="80" Description="TCP port 80" Type="Port" Mode="tcp" Display="always" Required="true" Mask="false"/>
    <Config Name="DB_DIR" Target="DB_DIR" Default="/db" Description="DB_DIR env var" Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
