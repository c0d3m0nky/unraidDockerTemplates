<?xml version="1.0"?>
    <Container version="1">
    <Name>taskerpy</Name>
    <Repository>c0d3m0nk3y/taskerpy</Repository>
<!--    <Registry>https://hub.docker.com/r/lloesche/valheim-server</Registry>-->
<!--    <Network>bridge</Network>-->
    <Network Default="bridge">bridge</Network>
    <Privileged>false</Privileged>
    <!-- <Support></Support> -->
    <Project>https://github.com/c0d3m0nky/unraidDockerTemplates</Project>
    <Overview>Python dynamic API</Overview>
    <!-- <WebUI></WebUI> -->
    <TemplateURL>https://raw.githubusercontent.com/c0d3m0nky/unraidDockerTemplates/main/templates/taskerpy.xml</TemplateURL>
    <Icon>https://raw.githubusercontent.com/c0d3m0nky/unraidDockerTemplates/a82389099fc7499e546e190e728e1cb4add49c58/images/taskerpy.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <Config Name="Code" Target="/code" Default="" Mode="rw" Description="Path to source code" Type="Path" Display="always" Required="false" Mask="false"/>
    <Config Name="HTTP" Target="80" Description="TCP port 80" Type="Port" Mode="tcp" Display="always" Required="true" Mask="false"/>
    <Config Name="Postgres Host" Target="PGHOST" Default="" Description="Postgres hostname/ip" Type="Variable" Display="always" Required="true" Mask="false"/>
    <Config Name="Postgres Port" Target="PGPORT" Default="5432" Description="Postgres port" Type="Variable" Display="always" Required="true" Mask="false"/>
    <Config Name="Postgres User" Target="PGUSER" Default="" Description="Postgres user" Type="Variable" Display="always" Required="true" Mask="false"/>
    <Config Name="Postgres Password" Target="PGPWD" Default="" Description="Postgres user password" Type="Variable" Display="always" Required="true" Mask="true"/>
</Container>